{
  "name": "ripaclub/zf2-hanger-snippet",
  "type": "library",
  "description": "A module to attach HTML/JS snippets into a ZF2 application",
  "homepage": "https://github.com/ripaclub/zf2-hanger-snippet",
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Lorenzo Fontana",
      "email": "fontanalorenzo@me.com",
      "homepage": "https://github.com/fntlnz"
    },
    {
      "name": "Leonardo Grasso",
      "email": "me@leonardograsso.com",
      "homepage": "https://github.com/leogr"
    },
    {
      "name": "Leo Di Donato",
      "email": "leodidonato@gmail.com",
      "homepage": "http://github.com/leodido"
    }
  ],
  "require": {
    "php": ">=5.4",
    "zendframework/zend-view": "2.*",
    "zendframework/zend-servicemanager": "2.*",
    "zendframework/zend-filter": "2.*",
    "zendframework/zend-modulemanager": "2.*",
    "zendframework/zend-escaper": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3"
  },
  "autoload": {
    "psr-4": {
      "HangerSnippet\\": "src"
    },
    "classmap": [
      "./Module.php"
    ]
  },
  "autoload-dev": {
    "prs-4": {
      "HangerSnippetTest\\": "tests"
    }
  },
  "keywords": [
    "zf2",
    "hanger",
    "snippets",
    "js",
    "javascript",
    "appender",
    "snippet",
    "append",
    "google analytics",
    "facebook",
    "universal analytics",
    "zend framework"
  ]
}